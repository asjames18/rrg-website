{
  "name": "rrg-website",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": "18.x || 20.x"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "vercel:pull": "vercel pull --yes --environment=production",
    "vercel:build": "vercel build",
    "vercel:deploy:prod": "vercel deploy --prebuilt --prod",
