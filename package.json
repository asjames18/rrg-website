    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@vercel/analytics": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.4.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.7.2",
    "validator": "^13.15.23",
    "web-push": "^3.6.7",
    "workflow": "^4.0.1-beta.13",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@astrojs/vercel": "^8.0.4",
