{
  "name": "rrg-website",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": "18.x || 20.x"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "prebuild": "bash scripts/check-no-legacy.sh",
