    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.0.0-alpha.30",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@astrojs/vercel": "^8.2.8",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "astro": "^5.14.1"
  }
